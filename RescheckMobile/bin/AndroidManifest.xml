<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="team1.project2.rescheckmobile"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:screenOrientation="portrait">
        <!-- Home activity -->
        <activity android:name="team1.project2.rescheckmobile.HomeActivity"
            android:screenOrientation="portrait"/>
        
        <!-- Project activity -->
        <activity android:name="team1.project2.rescheckmobile.ProjectActivity"
            android:screenOrientation="portrait"/>
        <!-- Envelope activity -->
        <activity android:name="team1.project2.rescheckmobile.EnvelopeActivity"
            android:screenOrientation="portrait"/>
        
        <!-- Mechanical activity -->
        <activity android:name="team1.project2.rescheckmobile.MechanicalActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="team1.project2.rescheckmobile.MainActivity"
            android:label="@string/app_name" 
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        
        
    </application>

</manifest>